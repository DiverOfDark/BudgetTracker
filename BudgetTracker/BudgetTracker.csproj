<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <PreserveCompilationContext>true</PreserveCompilationContext>
    <MvcRazorCompileOnPublish>true</MvcRazorCompileOnPublish>
    <LangVersion>8</LangVersion>
    <RuntimeFrameworkVersion>2.2.4</RuntimeFrameworkVersion>
    <TrimUnusedDependencies>true</TrimUnusedDependencies>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Hangfire.AspNetCore" Version="1.7.2" />
    <PackageReference Include="JetBrains.Annotations" Version="2019.1.1" />
    <PackageReference Include="Microsoft.AspNetCore.All" Version="2.2.5" />
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp.Scripting" Version="3.1.0" />
    <PackageReference Include="Microsoft.Packaging.Tools.Trimming" Version="1.1.0-prerelease.18603.1" />
    <PackageReference Include="Octokit" Version="0.32.0" />
    <PackageReference Include="OutCode.EscapeTeams.ObjectRepository.AzureTableStorage" Version="1.0.124" />
    <PackageReference Include="OutCode.EscapeTeams.ObjectRepository.LiteDB" Version="1.0.124" />
    <PackageReference Include="OutCode.EscapeTeams.ObjectRepository.Hangfire" Version="1.0.124" />
    <PackageReference Include="Selenium.Support" Version="3.141.0" />
    <PackageReference Include="Selenium.WebDriver" Version="3.141.0" />
    <PackageReference Include="Selenium.WebDriver.ChromeDriver" Version="74.0.3729.6" />
    <PackageReference Include="System.Text.Encoding.CodePages" Version="4.5.1" />
    <PackageReference Include="System.Text.Encodings.Web" Version="4.5.0" />
  </ItemGroup>
  <ItemGroup>
    <Content Update="electron.manifest.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <_ContentIncludedByDefault Remove="Views\SmsList\Index.cshtml" />
    <_ContentIncludedByDefault Remove="Views\SmsList\SmsRules.cshtml" />
    <_ContentIncludedByDefault Remove="Views\Payment\EditCategory.cshtml" />
    <_ContentIncludedByDefault Remove="Views\Payment\EditPayment.cshtml" />
    <_ContentIncludedByDefault Remove="Views\Payment\Index.cshtml" />
    <_ContentIncludedByDefault Remove="Views\Payment\SpentCategories.cshtml" />
    <_ContentIncludedByDefault Remove="Views\Payment\SplitPayment.cshtml" />
  </ItemGroup>
</Project>